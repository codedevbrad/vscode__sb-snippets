{
    "name": "sbsnippets",
    "displayName": "sbsnippets",
    "description": "snippets for storybook react",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Snippets"
    ],
    "scripts": {
        "del":  "del-cli --force ./dev/sbsnippets",
        "copy": "robocopy ./ /mir ./dev/sbsnippets /xd dev node_modules",
        "open": "node dev/scripts/open" ,

        "pack": "npm run open && npm run del && npm run copy"
    },
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.template-snippets.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.template-snippets.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.template-snippets.json"
            },
            {
                "language": "html",
                "path": "./snippets/snippets.minor-snippets.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.minor-snippets.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.minor-snippets.json"
            }
        ]
    },
    "dependencies": {
        "del-cli": "^4.0.1"
    }
}
